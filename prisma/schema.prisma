generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @unique
  createdAt DateTime @default(now())
  stage     Int      @default(0)
  finished  Boolean  @default(false)
  lastest   DateTime @default(now())
}
